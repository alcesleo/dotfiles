[alias]
    st = status
    ci = commit
    co = checkout
    cl = clone
    br = branch
    di = diff
    ds = diff --staged
    ff = pull --ff-only

    amend     = commit --amend
    tree      = log --graph --oneline --decorate --all
    yolo      = push --force
    forget    = update-index --assume-unchanged
    remember  = update-index --no-assume-unchanged
    changelog = !git log --oneline `git describe --abbrev=0`..HEAD
    supdate   = submodule update --init --recursive

[core]
    excludesfile = ~/.gitignore

[color]
    diff   = auto
    status = auto
    branch = auto
    ui = true

[push]
    default = current

[credential]
    helper = osxkeychain
